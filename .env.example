# Copy this file to .env and fill in your actual values
# DO NOT commit the .env file to git!

# Required: Your Argo Workflows bearer token
ARGO_BEARER_TOKEN=your-token-here

# Optional: Override defaults
# ARGO_API_URL=https://workflows.argocd.domain/api/v1/workflows/default
# ARGO_NAMESPACE=default
# ARGO_WORKFLOW_LIMIT=1000

# S3 Configuration (optional - for uploading reports to S3)
# S3_BUCKET=your-bucket-name
# S3_PREFIX=argo-reports/
# AWS_REGION=eu-west-1

